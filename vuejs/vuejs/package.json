{
  "name": "vuejs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-s dev:*",
    "dev:build": "vue-cli-service build",
    "dev:serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "docker:build": "docker build -t vuejs-app .",
    "docker:deploy": "docker run -d -p 8080:80 --rm --name vuejs-app vuejs-app"
  },
  "dependencies": {
    "ci": "^2.2.0",
    "core-js": "^3.14.0",
    "vue": "^2.6.14",
    "vuejs": "^3.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@vue/cli-plugin-babel": "^5.0.1",
    "@vue/cli-plugin-eslint": "^5.0.1",
    "@vue/cli-service": "^5.0.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.19.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4",
    "vue-template-compiler": "^2.6.14"
  },
  "resolutions": {
    "glob-parent": "5.1.2",
    "set-value": "4.0.1",
    "nth-check": "2.0.1",
    "ansi-regex": "5.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
